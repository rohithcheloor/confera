{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/server.bundle.cjs"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "PORT": 8080
    }
  }
  